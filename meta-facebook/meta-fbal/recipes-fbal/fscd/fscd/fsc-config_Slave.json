{
    "version": "fbal-r08",
    "pwm_transition_value": 70,
    "pwm_boost_value": 100,
    "sample_interval_ms": 3000,
    "fan_recovery_time": 2,
    "sensor_filter_all": true,
    "boost": {
      "fan_fail": true,
      "sensor_fail": false
    },
    "sensor_valid_check": {
      "mb": {
        "attribute": {
          "type" : "power_status"
        }
      }
    },
    "watchdog": false,
    "min_rpm": 600,
    "profiles": {
      "linear_mezz": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "positive_hysteresis": 0,
        "negative_hysteresis": 2,
        "data": [
          [80, 15],
          [83, 18],
          [85, 21],
          [87, 25],
          [90, 40],
          [95, 70]
        ]
      },
      "linear_inlet": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "positive_hysteresis": 0,
        "negative_hysteresis": 2,
        "data": [
          [25, 13],
          [30, 15],
          [35, 18],
          [38, 50],
          [40, 80]
        ]
      },
      "linear_vr": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "positive_hysteresis": 0,
        "negative_hysteresis": 2,
        "data": [
          [70, 15],
          [75, 20],
          [80, 25],
          [85, 35],
          [90, 60],
          [95, 65]
        ]
      }
    },
    "fans": {
      "0": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 0"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d"
        }
      },
      "1": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 1"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d"
        }
      },
      "2": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 2"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d"
        }
      },
      "3": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 3"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d"
        }
      },
      "4": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 4"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d"
        }
      },
      "5": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 5"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d"
        }
      },
      "6": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 6"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d"
        }
      },
      "7": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 7"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d"
        }
      }
    },
    "zones": {
      "zone_1": {
        "pwm_output": [0],
        "expr_file": "zone1_Slave.fsc"
      }
    }
}
